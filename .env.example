# HMAS Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# Gemini API Configuration (M6 Neural Link)
# =============================================================================

# Your Gemini API key from Google AI Studio
# Get yours at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Gemini model to use (default: gemini-1.5-pro)
# Options: gemini-1.5-pro, gemini-1.5-flash, gemini-2.0-flash-exp
GEMINI_MODEL=gemini-1.5-pro

# =============================================================================
# Optional Configuration
# =============================================================================

# HMAS log level (DEBUG, INFO, WARNING, ERROR)
# HMAS_LOG_LEVEL=INFO
